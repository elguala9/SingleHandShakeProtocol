{
  "name": "@shsp/types",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "barrelsby": "barrelsby --directory src --delete --barrelName index.ts --extensions ts",
    "build": "tsc -b",
    "gen-exports": "npx gen-exports dist -p package.json"
  },
  "exports": {
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ShspRockTypes": {
      "import": "./dist/ShspRockTypes.js",
      "require": "./dist/ShspRockTypes.js",
      "types": "./dist/ShspRockTypes.d.ts"
    },
    "./StunTypes": {
      "import": "./dist/StunTypes.js",
      "require": "./dist/StunTypes.js",
      "types": "./dist/StunTypes.d.ts"
    }
  }
}
